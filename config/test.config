/*
 * -------------------------------------------------
 *  Nextflow config file for running tests
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a fast and simple test. Use as follows:
 *   nextflow run umi-pipeline-nf -profile test
 */

params {

    help = false
	version = false
	debug = false
	
	input = "umi-pipeline-nf/data/example_egfr_single_cluster.fastq"
	output = "test_pipeline"
	reference = "umi-pipeline-nf/data/example_egfr_reference.fasta"
	bed = "umi-pipeline-nf/data/example_egfr_amplicon.bed"

}